{
	"name": "yae-bot",
	"description": "A Discord bot powered by Seyfert, focused on developer experience and utility functions.",
	"author": "OuOSama",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "^2.3.6",
		"@types/bun": "^1.3.2",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"riffy": "^1.0.8",
		"seyfert": "^4.0.0"
	},
	"scripts": {
		"dev": "bun run ./src/app.ts",
		"start": "bun run ./src/app.js",
		"lavalink-setup": "docker-compose --env-file ./.env.local -f ./lavalink/compose.yml up -d",
		"check": "bunx biome check --write",
		"prepare": "bunx husky",
		"remove-commands": "bun run ./src/script/remove_commandst.ts"
	},
	"lint-staged": {
		"*.{ts,json,d.ts}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"license": "MIT"
}
